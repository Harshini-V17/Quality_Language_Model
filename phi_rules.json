{
  "patterns": [
    {
      "type": "doctor_name",
      "pattern": "Dr\\.\\s?[A-Za-z .]+"
    },
    {
      "type": "patient_name",
      "pattern": "Patient Name:\\s?[A-Za-z ]+"
    },
    {
      "type": "phone_number",
      "pattern": "\\+91-[0-9]{10}"
    },
    {
      "type": "mrn",
      "pattern": "MRN[0-9]+"
    },
    {
      "type": "address",
      "pattern": "Address:\\s?[0-9, A-Za-z]+"
    }
  ],
  "redaction_format": "[REDACTED_{type}]",
  "version": "v1.0",
  "notes": "Basic regex rules for PHI redaction"
}